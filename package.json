{
  "name": "npm-test",
  "version": "1.0.0",
  "description": "Just testing npm and node",
  "main": "index.js",
  "scripts": {
    "test": "mocha test -u bdd -R spec",
    "start": "node index.js",
    "start:dev": "node index.js 4000",
    "lint": "jshint npm-test",
    "compile:coffee": "coffee --compile --output ./lib ./src/coffeescripts",
    "compile": "npm run compile:coffee",
    "precompile": "npm run clean",
    "clean": "rimraf lib/**",
    "build:less": "lessc client/less/demo.less public/css/site.css",
    "build:bundle": "browserify ./client/js/app.js -o ./public/js/bundle.js"
  },
  "jshintConfig": {
    "esnext": "true",
    "noyield": "true"
  },
  "Keywords": [
    "npm",
    "scripting"
  ],
  "author": "Jesper Nørløv Arnholtz",
  "license": "MIT",
  "devDependencies": {
    "browserify": "^12.0.1",
    "coffee-script": "^1.10.0",
    "jshint": "^2.8.0",
    "less": "^2.5.3",
    "mocha": "^2.3.3",
    "rimraf": "^2.4.3",
    "should": "^7.1.1",
    "supertest": "^1.1.0"
  },
  "dependencies": {
    "koa": "^1.1.1"
  }
}
